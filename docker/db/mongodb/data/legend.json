{
    "nombre": "legend",
    "descripcion": "Esta etiqueta se usa para definir un fragmento de código de computadora. El contenido del interior se muestra en la fuente mono espaciada predeterminada del navegador.",
    "ejemplo": [
        "<legend>Titulo Fieldset</legend>"
    ],
    "vacio": false,
    "categoria": "Formularios",
    "atributo": {
        "accesskey": {
            "descripcion": "Carácter de la tecla de acceso rápido.",
            "booleano": false,
            "valor": "Valor Entero",
            "ejemplo": [
                "<legend accesskey=\"1\"></legend>"
            ]
        },
        "align": {
            "descripcion": "Alineación con relación al fieldset.",
            "booleano": false,
            "valor": [
                "top",
                "bottom",
                "left",
                "right"
            ],
            "ejemplo": [
                "<legend align=\"top\"></legend>"
            ]
        }
    }
}